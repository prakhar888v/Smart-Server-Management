PROCEDURE "SAP_SRVMGR"."SRVMANAGER::SQL_GET_MONDATA" ( )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
SELECT * FROM (
    SELECT TOP 100 *
    FROM "SAP_SRVMGR"."SRVMON_MASTER" WHERE "DataId" IS NOT NULL ORDER BY "Timestamp" desc)
order by "Timestamp" asc;
END